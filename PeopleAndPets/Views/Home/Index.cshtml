@model PetOwners
@{
    ViewBag.Title = "Home Page";
}


<div class="container">
    <div class="panel panel-default">
        <div class="panel-heading">Cats grouped by owner gender !</div>
        <table class="table">
            <thead>
                <tr>
                    <th>Male</th>
                    <th>Female</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <ul class="list-group">
                           
                            @foreach (var item in Model.PetsOwnedByMale)
                            {
                                <li class="list-group-item">
                                    @Html.DisplayFor(modeltItem => item.Name)
                                </li>
                            }
                        </ul>


                    </td>
                    <td>
                        <ul class="list-group">
                            @foreach (var item in Model.PetsOwnedByFemale)
                            {
                                <li class="list-group-item">
                                    @Html.DisplayFor(modelItem => item.Name)
                                </li>
                            }
                        </ul>

                    </td>
                </tr>

            </tbody>
        </table>
    </div>
</div>